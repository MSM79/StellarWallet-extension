{
  "manifest_version": 2,
  "name": "Rabet",
  "version": "1.7.1",
  "description": "Rabet extension, a simple way to interact with Stellar network.",
  "homepage_url": "https://rabet.io",
  "icons": {},
  "browser_action": {
    "default_title": "Rabet",
    "default_popup": "dist/popup.html"
  },
  "permissions": ["storage", "tabs"],
  "web_accessible_resources": ["dist/*.*"]
}
